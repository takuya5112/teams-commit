<!DOCTYPE html>
<html>
  <head>
    <title>TeamsCommit</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
   <%= javascript_include_tag 'application'%>
  </head>
  <body>
    <% if user_signed_in? %>
    <header>
      <div class= "header">
        <div class= "header-left">
          <%= link_to "TeamsCommit", root_path, class: "app-title" %>
        </div>
      </div>
    </header>
  <% end %>
  <main>
    <div class= "flash-items">
      <%= render 'layouts/notifications' %>
      <%= image_tag "/assets/image3.png", class: "flash-icon"%>
    </div>
    <%= yield %>
  </main>
 </body>
</html>
